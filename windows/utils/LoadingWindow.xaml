<Window x:Class="projektlabor.covid19login.adminpanel.windows.utils.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projektlabor.covid19login.adminpanel.windows.utils"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Background="#7A7A7A"
        xmlns:lang="clr-namespace:projektlabor.covid19login.adminpanel.Properties.langs"
        xmlns:uc="clr-namespace:projektlabor.covid19login.adminpanel.uiElements"
        Title="Title"
        Height="450" Width="450" WindowStyle="None">
    <DockPanel>
        <TextBlock x:Name="textdisplay" Foreground="White" DockPanel.Dock="Top" HorizontalAlignment="Center" FontFamily="Consolas" FontSize="20" Margin="10" Text="Text"/>
        <uc:LoadingAnimation DockPanel.Dock="Bottom"/>
    </DockPanel>
</Window>
