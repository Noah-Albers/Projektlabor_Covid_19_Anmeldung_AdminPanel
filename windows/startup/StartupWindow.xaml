<Window x:Class="projektlabor.covid19login.adminpanel.windows.startup.StartupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Background="#7A7A7A"
        xmlns:lang="clr-namespace:projektlabor.covid19login.adminpanel.Properties.langs"
        Title="{x:Static lang:Lang.startup_title}"
        Loaded="OnWindowLoaded"
        xmlns:uc="clr-namespace:projektlabor.covid19login.adminpanel.uiElements"
        Height="450" Width="450">
    <Grid>
        <uc:LoadingAnimation/>
    </Grid>
</Window>
